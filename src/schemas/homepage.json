{
  "HomePage": {
    "home_page_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "home page title",
        "placeholder": "Enter home page title"
      }
    }
  },
  "contacts": {
    "contacts_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading2",
        "label": "contacts title",
        "placeholder": "enter contacts title"
      }
    },
    "contacts_subtitle": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "allowTargetBlank": true,
        "label": "contacts subTitle",
        "placeholder": "enter sub title"
      }
    },
    "contacts_content": {
      "type": "Group",
      "config": {
        "fields": {
          "list_item_name": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph",
              "label": "list item name",
              "placeholder": "name of contact"
            }
          },
          "list_item_content": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph",
              "label": "list item content",
              "placeholder": "contact content"
            }
          }
        },
        "label": "contacts content"
      }
    },
    "list_social_links": {
      "type": "Group",
      "config": {
        "fields": {
          "social_logo": {
            "type": "Image",
            "config": {
              "constraint": {},
              "thumbnails": [],
              "label": "social logo"
            }
          },
          "social_links": {
            "type": "Link",
            "config": {
              "label": "social links"
            }
          }
        },
        "label": "list social links"
      }
    }
  },
  "portfolio": {
    "portfolio_field": {
      "type": "Group",
      "config": {
        "fields": {
          "portfolio_card_image": {
            "type": "Image",
            "config": {
              "constraint": {},
              "thumbnails": [],
              "label": "portfolio card image"
            }
          },
          "card_background_color": {
            "type": "Color",
            "config": {
              "label": "Card background color"
            }
          },
          "portfolio_card_title": {
            "type": "StructuredText",
            "config": {
              "single": "heading1",
              "label": "portfolio card title",
              "placeholder": "Enter card title"
            }
          },
          "portfolio_card_meta_tegs": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph",
              "label": "portfolio card meta tegs",
              "placeholder": "Enter meta tags with a space"
            }
          },
          "portfolio_card_description": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph",
              "label": "portfolio card description",
              "placeholder": "Enter card description"
            }
          }
        },
        "label": "portfolio field"
      }
    }
  }
